<%- include('partials/header'); %>
    <div class="relative md:ml-64 bg-blueGray-50">
        <link href="./assets/css/nucleo-icons.css" rel="stylesheet" />
        <link href="./assets/css/nucleo-svg.css" rel="stylesheet" />
        <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet" />
        <link rel="stylesheet" href="styles.css" />
        <script src="../path/to/soft-ui-dashboard-tailwind.js"></script>
        <script src="https://kit.fontawesome.com/42d5adcbca.js" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@9"></script>
        <!-- Header -->

        <section class="pt-10 py-24 lg:pt-0 bg-gray-200 flex flex-wrap">
            <form action="/agregar" method="POST" class="flex flex-wrap">
                <div class="w-full lg:w-6/12 xl:w-2/12 px-4">
                    <div class="relative flex flex-col min-w-0 break-words bg-white rounded mb-6 xl:mb-0 shadow-lg">
                        <div class="flex-auto p-5 lg:p-10">
                            <h4 class="text-2xl font-semibold">Quieres Colegiarte?</h4>
                            <p class="leading-relaxed mt-1 mb-4 text-gray-600">
                                Completa el siguente formulario con tus datos.
                            </p>
                        

                            <div class="relative w-full mb-3 mt-8">
                                <label class="mb-2 ml-1 font-bold text-size-xs text-slate-700 dark:text-white/80"
                                    for="First Name">Nombres</label>
                                <input type="text" name="nombre" placeholder="Eg. Michael"
                                    class="mb-4 focus:shadow-soft-primary-outline dark:bg-gray-950 dark:placeholder:text-white/80 dark:text-white/80 text-size-sm leading-5.6 ease-soft block w-full appearance-none rounded-lg border border-solid border-gray-300 bg-white bg-clip-padding px-3 py-2 font-normal text-gray-700 outline-none transition-all placeholder:text-gray-500 focus:border-fuchsia-300 focus:outline-none" />
                            </div>
                            <div class="relative w-full mb-3 mt-8">
                                <label class="mb-2 ml-1 font-bold text-size-xs text-slate-700 dark:text-white/80"
                                    for="full-name">Apellido Paterno </label>
                                <input type="text" name="apellidop" placeholder="Eg. Tomson"
                                    class="mb-4 focus:shadow-soft-primary-outline dark:bg-gray-950 dark:placeholder:text-white/80 dark:text-white/80 text-size-sm leading-5.6 ease-soft block w-full appearance-none rounded-lg border border-solid border-gray-300 bg-white bg-clip-padding px-3 py-2 font-normal text-gray-700 outline-none transition-all placeholder:text-gray-500 focus:border-fuchsia-300 focus:outline-none" />
                            </div>
                            <div class="relative w-full mb-3 mt-8">
                                <label class="mb-2 ml-1 font-bold text-size-xs text-slate-700 dark:text-white/80"
                                    for="full-name">Apellido Materno</label>
                                <input type="text" name="apellidom" placeholder="Eg. Tomson"
                                    class="mb-4 focus:shadow-soft-primary-outline dark:bg-gray-950 dark:placeholder:text-white/80 dark:text-white/80 text-size-sm leading-5.6 ease-soft block w-full appearance-none rounded-lg border border-solid border-gray-300 bg-white bg-clip-padding px-3 py-2 font-normal text-gray-700 outline-none transition-all placeholder:text-gray-500 focus:border-fuchsia-300 focus:outline-none" />
                            </div>
                            <div class="relative w-full mb-3 mt-8">
                                <label class="mb-2 ml-1 font-bold text-size-xs text-slate-700 dark:text-white/80"
                                    for="full-name">DNI</label>
                                <input type="text" name="dni" placeholder="Documento"
                                    class="mb-4 focus:shadow-soft-primary-outline dark:bg-gray-950 dark:placeholder:text-white/80 dark:text-white/80 text-size-sm leading-5.6 ease-soft block w-full appearance-none rounded-lg border border-solid border-gray-300 bg-white bg-clip-padding px-3 py-2 font-normal text-gray-700 outline-none transition-all placeholder:text-gray-500 focus:border-fuchsia-300 focus:outline-none" />
                            </div>
                            <div class="relative w-full mb-3 mt-8">
                                <label class="mb-2 ml-1 font-bold text-size-xs text-slate-700 dark:text-white/80"
                                    for="celular">Celular</label>
                                <input type="text" name="celular" placeholder="+51 987654234"
                                    class="mb-4 focus:shadow-soft-primary-outline dark:bg-gray-950 dark:placeholder:text-white/80 dark:text-white/80 text-size-sm leading-5.6 ease-soft block w-full appearance-none rounded-lg border border-solid border-gray-300 bg-white bg-clip-padding px-3 py-2 font-normal text-gray-700 outline-none transition-all placeholder:text-gray-500 focus:border-fuchsia-300 focus:outline-none" />

                            </div>
                            <div class="relative w-full mb-3">
                                <label class="mb-2 ml-1 font-bold text-size-xs text-slate-700 dark:text-white/80"
                                    for="email">Email</label>
                                <input type="email" name="email" placeholder="CIP@gmail.com"
                                    class="mb-4 focus:shadow-soft-primary-outline dark:bg-gray-950 dark:placeholder:text-white/80 dark:text-white/80 text-size-sm leading-5.6 ease-soft block w-full appearance-none rounded-lg border border-solid border-gray-300 bg-white bg-clip-padding px-3 py-2 font-normal text-gray-700 outline-none transition-all placeholder:text-gray-500 focus:border-fuchsia-300 focus:outline-none" />
                            </div>
                            <div class="relative w-full mb-3">
                                <label class="mb-2 ml-1 font-bold text-size-xs text-slate-700 dark:text-white/80"
                                    for="Especialidad">Especialidad</label>
                                <select choice
                                    class="focus:shadow-soft-primary-outline dark:bg-gray-950 dark:placeholder:text-white/80 dark:text-white/80 text-size-sm leading-5.6 ease-soft block w-full appearance-none rounded-lg border border-solid border-gray-300 bg-white bg-clip-padding px-3 py-2 font-normal text-gray-700 outline-none transition-all placeholder:text-gray-500 focus:border-fuchsia-300 focus:outline-none"
                                    name="especialidad" id="especialidad">
                                    <option value="ING.Sistemas y Cómputo">ING.Sistemas y Cómputo</option>
                                    <option value="ING.Civil">ING.Civil</option>
                                    <option value="ING.Mecánica">ING.Mecánica</option>
                                    <option value="ING.Industrial">ING.Industrial</option>
                                    <option value="ING.Eléctrico">ING.Eléctrico</option>
                                </select>
                            </div>


                        </div>
                    </div>
                </div>
                <div class="w-full lg:w-6/12 xl:w-2/12 px-4">
                    <div class="relative flex flex-col min-w-0 break-words bg-white rounded mb-6 xl:mb-0 shadow-lg">
                        <div class="flex-auto p-5 lg:p-10">
                            <h4 class="text-2xl font-semibold">Adjunte documentos</h4>
                            <div class="relative w-full mb-3">
                                <label class="leading-relaxed mt-1 mb-4 text-gray-600" for="email">Adjunte DNI (anverso
                                    y reverso).</label><button
                                    class="bg-gray-900 text-white active:bg-gray-700 text-sm font-bold uppercase px-6 py-3 rounded shadow hover:shadow-lg outline-none focus:outline-none mr-1 mb-1"
                                    type="button" style="transition: all 0.15s ease 0s;">
                                    Adjuntar documentos
                                    <input type="file" name="docd" multiple>

                                </button>
                            </div>
                            <div class="relative w-full mb-3">
                                <label class="leading-relaxed mt-1 mb-4 text-gray-600" for="email">Adjunte TÍTULO
                                    UNIVERSITARIO (anverso y reverso).</label><button
                                    class="bg-gray-900 text-white active:bg-gray-700 text-sm font-bold uppercase px-6 py-3 rounded shadow hover:shadow-lg outline-none focus:outline-none mr-1 mb-1"
                                    type="button" style="transition: all 0.15s ease 0s;">
                                    Adjuntar documentos
                                    <input type="file" name="doct" multiple>

                                </button>
                            </div>
                            <div class="relative w-full mb-3">
                                <label class="leading-relaxed mt-1 mb-4 text-gray-600" for="email">Adjunte CONSTANCIA DE
                                    LA SUNEDU </label><button
                                    class="bg-gray-900 text-white active:bg-gray-700 text-sm font-bold uppercase px-6 py-3 rounded shadow hover:shadow-lg outline-none focus:outline-none mr-1 mb-1"
                                    type="button" style="transition: all 0.15s ease 0s;">
                                    Adjuntar documentos
                                    <input type="file" name="docc" multiple>

                                </button>
                            </div>
                            <div class="relative w-full mb-3">
                                <label class="leading-relaxed mt-1 mb-4 text-gray-600" for="email">Adjunte FIRMA DEL
                                    INGENIERO A COLEGIARSE</label><button
                                    class="bg-gray-900 text-white active:bg-gray-700 text-sm font-bold uppercase px-6 py-3 rounded shadow hover:shadow-lg outline-none focus:outline-none mr-1 mb-1"
                                    type="button" style="transition: all 0.15s ease 0s;">
                                    Adjuntar documentos
                                    <input type="file" name="docf" multiple>

                                </button>
                            </div>
                            <div class="relative w-full mb-3">
                                <label class="leading-relaxed mt-1 mb-4 text-gray-600" for="email">Adjunte FOTO DIGITAL
                                    A COLOR</label><button
                                    class="bg-gray-900 text-white active:bg-gray-700 text-sm font-bold uppercase px-6 py-3 rounded shadow hover:shadow-lg outline-none focus:outline-none mr-1 mb-1"
                                    type="button" style="transition: all 0.15s ease 0s;">
                                    Adjuntar documentos
                                    <input type="file" name="docft" multiple>

                                </button>
                            </div>

                        </div>

                     </div>
                     <div
                        class="relative flex flex-col min-w-0 break-words bg-white rounded mb-6 xl:mb-0 shadow-lg mt-5">
                        <div class="flex-auto p-10 lg:p-10">


                            <div class="relative w-full mb-3">


                                <input type="submit" href=""
                                    class="bg-gray-900 text-white active:bg-gray-700 text-sm font-bold uppercase px-6 py-3 rounded shadow hover:shadow-lg outline-none focus:outline-none mr-1 mb-1"
                                    style="transition: all 0.15s ease 0s;" value="Enviar datos">

                            </div>

                        </div>

                    </div>

                </div>
            </form>


        </section>


    </div>
    <%- include('partials/footer'); %>